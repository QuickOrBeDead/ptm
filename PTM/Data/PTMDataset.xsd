<?xml version="1.0" standalone="yes" ?>
<xs:schema id="PTMDataset" targetNamespace="http://www.tempuri.org/PTMDataset.xsd" xmlns:mstns="http://www.tempuri.org/PTMDataset.xsd"
	xmlns="http://www.tempuri.org/PTMDataset.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
	<xs:element name="PTMDataset" msdata:IsDataSet="true">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="TasksLog">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Id" msdata:AutoIncrement="true" type="xs:int" />
							<xs:element name="TaskId" type="xs:int" minOccurs="0" />
							<xs:element name="Duration" type="xs:int" minOccurs="0" />
							<xs:element name="InsertTime" type="xs:dateTime" minOccurs="0" />
							<xs:element name="UpdateTime" type="xs:dateTime" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="ApplicationsLog">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Id" msdata:AutoIncrement="true" type="xs:int" />
							<xs:element name="ProcessId" type="xs:int" minOccurs="0" />
							<xs:element name="TaskLogId" type="xs:int" minOccurs="0" />
							<xs:element name="Name" type="xs:string" minOccurs="0" />
							<xs:element name="Caption" type="xs:string" minOccurs="0" />
							<xs:element name="ApplicationFullPath" type="xs:string" minOccurs="0" />
							<xs:element name="LastUpdateTime" type="xs:dateTime" minOccurs="0" />
							<xs:element name="UserProcessorTime" type="xs:int" minOccurs="0" />
							<xs:element name="ActiveTime" type="xs:int" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Tasks">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="DefaultTaskId" type="xs:int" minOccurs="0" />
							<xs:element name="Description" type="xs:string" minOccurs="0" />
							<xs:element name="Id" msdata:AutoIncrement="true" type="xs:int" />
							<xs:element name="IsDefaultTask" type="xs:boolean" minOccurs="0" />
							<xs:element name="IsFinished" type="xs:boolean" minOccurs="0" />
							<xs:element name="ParentId" type="xs:int" minOccurs="0" />
							<xs:element name="StartDate" type="xs:dateTime" minOccurs="0" />
							<xs:element name="StopDate" type="xs:dateTime" minOccurs="0" />
							<xs:element name="TotalTime" type="xs:int" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
		<xs:unique name="PTMDatasetKey2" msdata:PrimaryKey="true">
			<xs:selector xpath=".//mstns:TasksLog" />
			<xs:field xpath="mstns:Id" />
		</xs:unique>
		<xs:unique name="PTMDatasetKey4" msdata:PrimaryKey="true">
			<xs:selector xpath=".//mstns:ApplicationsLog" />
			<xs:field xpath="mstns:Id" />
		</xs:unique>
		<xs:unique name="Constraint1" msdata:PrimaryKey="true">
			<xs:selector xpath=".//mstns:Tasks" />
			<xs:field xpath="mstns:Id" />
		</xs:unique>
		<xs:keyref name="TasksTasks" refer="Constraint1" msdata:DeleteRule="Cascade">
			<xs:selector xpath=".//mstns:Tasks" />
			<xs:field xpath="mstns:ParentId" />
		</xs:keyref>
		<xs:keyref name="TasksLogApplicationsLog" refer="PTMDatasetKey2" msdata:DeleteRule="Cascade">
			<xs:selector xpath=".//mstns:ApplicationsLog" />
			<xs:field xpath="mstns:TaskLogId" />
		</xs:keyref>
		<xs:keyref name="TasksTasksLog" refer="Constraint1" msdata:DeleteRule="Cascade">
			<xs:selector xpath=".//mstns:TasksLog" />
			<xs:field xpath="mstns:TaskId" />
		</xs:keyref>
	</xs:element>
</xs:schema>